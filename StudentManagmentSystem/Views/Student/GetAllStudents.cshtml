@model List<Student>
@{
    ViewData["Title"] = "GetAllStudents";
}
<h1>GetAllStudents</h1>
<table class="table table-bordered table-hover">
    <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Phone</th>
        <th>Class Level</th>
    


    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Email</td>          
            <td>@item.Phone </td>
            <td>@item.ClassLevel</td>
            <td><a href="/Student/Edit/@item.Id" class="btn btn-primary">Edit</a></td>
           
        </tr>
    }

</table>

